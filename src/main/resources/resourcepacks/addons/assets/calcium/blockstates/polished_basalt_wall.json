{
  "multipart": [
    { "when": { "up": "true" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_post" } 
	},
    { "when": { "north": "low", "south": "low" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_side1", "y": 180 } 
	},
    { "when": { "east": "low", "west": "low" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_side1", "y": 270 } 
	},
    { "when": { "north": "low", "south": "tall|none" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_side2" } 
	},
    { "when": { "east": "low", "west": "tall|none" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_side2", "y": 90 } 
	},
    { "when": { "south": "low", "north": "tall|none" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_side3" } 
	},
    { "when": { "west": "low", "east": "tall|none" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_side3", "y": 90 } 
	},
    { "when": { "north": "tall", "south": "tall" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_side_tall1", "y": 180 } 
	},
    { "when": { "east": "tall", "west": "tall" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_side_tall1", "y": 270 } 
	},
    { "when": { "north": "tall", "south": "low|none" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_side_tall2" } 
	},
    { "when": { "east": "tall", "west": "low|none" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_side_tall2", "y": 90 } 
	},
    { "when": { "south": "tall", "north": "low|none" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_side_tall3" } 
	},
    { "when": { "west": "tall", "east": "low|none" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_side_tall3", "y": 90 } 
	},
    { "when": { "north": "low", "south": "low", "east": "low", "west": "low" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_cross" } 
	},
    { "when": { "north": "tall", "south": "tall", "east": "tall", "west": "tall" }, 
	  "apply": { "model": "calcium:block/walls/polished_basalt_wall_cross_tall" } 
	}
  ]
}